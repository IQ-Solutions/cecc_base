{
  "name": "cecc_base",
  "version": "1.0.0",
  "description": "This sub-theme requires compiling of the USWDS CSS and also the assets folder for javascript.",
  "main": "index.js",
  "browserslist": [
    "last 2 versions",
    "> 1%"
  ],
  "postcss": {
    "map": "absolute",
    "plugins": {
      "cssnano": {
        "preset": "advanced"
      },
      "autoprefixer": {}
    }
  },
  "scripts": {
    "prebuild:css": "sass sass/styles.scss --include-path=./node_modules/uswds/src assets/css/uswds.css",
    "build:css": "postcss assets/css/uswds.css -o assets/css/uswds.min.css",
    "build": "npm run build:css",
    "build:watch": "watch 'npm run build' sass components",

    "storybook": "NODE_OPTIONS=\"--inspect\" start-storybook -p 3000",
    "build:dev": "webpack --config ./webpack.dev.js"
  },
  "author": "William Hurley",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/preset-typescript": "^7.16.7",
    "@drupal/once": "^1.0.1",
    "@storybook/addon-a11y": "^6.4.20",
    "@storybook/addon-essentials": "^6.4.20",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-viewport": "^6.4.20",
    "@storybook/builder-webpack5": "^6.4.20",
    "@storybook/html": "^6.4.20",
    "@storybook/manager-webpack5": "^6.4.20",
    "@types/jquery": "^3.5.14",
    "autoprefixer": "^10.4.4",
    "babel-plugin-iife-wrap": "^1.1.0",
    "babel-plugin-transform-remove-export": "^1.0.0",
    "babel-preset-minify": "^0.5.1",
    "buffer": "^6.0.3",
    "concurrently": "^7.1.0",
    "css-loader": "^6.2.0",
    "cssnano": "^5.1.7",
    "cssnano-preset-advanced": "^5.3.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^3.2.0",
    "glob": "^8.0.3",
    "jquery": "^3.6.0",
    "js-yaml-loader": "^1.2.2",
    "mini-css-extract-plugin": "^2.6.1",
    "onchange": "^7.1.0",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.1.0",
    "postcss-import-ext-glob": "^2.0.1",
    "postcss-load-config": "^3.1.4",
    "postcss-loader": "^6.1.1",
    "remove-files-webpack-plugin": "^1.5.0",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.49.11",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.0",
    "stylelint-webpack-plugin": "^3.3.0",
    "svg-to-png-loader": "^4.0.1",
    "ts-loader": "^9.3.1",
    "twig-html-loader": "0.1.9",
    "twigjs-loader": "1.0.2",
    "twing": "^5.1.1",
    "twing-drupal-filters": "^0.0.2",
    "twing-loader": "^4.0.0",
    "typescript": "^4.6.3",
    "uswds": "^2.13.3",
    "watch": "^1.0.2",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0",
    "yaml": "^2.1.1"
  }
}
